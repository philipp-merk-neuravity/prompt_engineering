{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Debug simple.py with Args",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/src/simple.py",
      "console": "integratedTerminal",
      "args": [
        "--model", "gpt-3.5-turbo-0125",
        "--prompt_type", "zero_shot_cot",
        "--benchmark_type", "all",
        "--chunk_size", "180",
        "--delay_seconds", "10"
      ],
      "cwd": "${workspaceFolder}/src"
    },
    {
      "name": "Debug reflection.py with Args",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/src/reflection.py",
      "console": "integratedTerminal",
      "args": [
        "--model_for_init", "gpt-3.5-turbo-0125",
        "--model_for_reflection", "gpt-3.5-turbo-0125",
        "--model_for_refinement", "gpt-3.5-turbo-0125",
        "--prompt_for_reflection", "reflexion_without_few_shot",
        "--prompt_for_refinement", "reflexion_without_few_shot",
        "--max_iterations", "4",
        "--benchmark_type", "all",
        "--chunk_size", "12",
        "--tests_path", "/home/neuravity/dev/prompt_engineering/src/human_eval/data/ExtractedTests.json"
      ],
      "cwd": "${workspaceFolder}/src"
    },
    {
      "name": "Python: Evaluate Functional Correctness",
      "type": "python",
      "request": "launch",
      "program": "/home/neuravity/dev/prompt_engineering/src/human_eval/human_eval/evaluate_functional_correctness.py",
      "args": [
          "/home/neuravity/dev/prompt_engineering/src/benchmark_results/all/reflection/standard/gpt-3/11/11.jsonl",
          "--problem_file=/home/neuravity/dev/prompt_engineering/src/human_eval/data/HumanEval.jsonl"
      ],
      "console": "integratedTerminal",
      "pythonPath": "/home/neuravity/dev/prompt_engineering/venv/bin/python",
      "env": {
          "PYTHONUNBUFFERED": "1"
      },
      "cwd": "${workspaceFolder}"
    }
  ]
}